(
  (ty a)
  (fun x0 a)
  (fun y0 a)
  (fun z0 a)
  (fun p0 bool)
  (fun x1 a)
  (fun y1 a)
  (fun z1 a)
  (fun p1 bool)
  (fun x2 a)
  (assert (not (= x0 x2)))
  (assert (or p0 (= x0 y0)))
  (assert (or p0 (= x1 y0)))
  (assert (or (not p0) (= x0 z0)))
  (assert (or (not p0) (= x1 z0)))
  (assert (or p1 (= x1 y1)))
  ;(assert (or p1 (= x2 y1)))
  (assert (or (not p1) (= x1 z1)))
  (assert (or (not p1) (= x2 z1)))
)
